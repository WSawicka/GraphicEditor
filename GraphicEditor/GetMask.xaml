        <TextBox x:Name="textBlock4" HorizontalAlignment="Left" Margin="97,49,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top"/>
        <TextBox x:Name="textBlock5" HorizontalAlignment="Left" Margin="173,52,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top"/>
        <TextBox x:Name="textBlock6" HorizontalAlignment="Left" Margin="33,78,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top"/>
        <TextBox x:Name="textBlock7" HorizontalAlignment="Left" Margin="97,78,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top"/>
