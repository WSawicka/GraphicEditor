        <Button x:Name="buttonLaplace" Content="Laplace" HorizontalAlignment="Left" Margin="10,189,0,0" VerticalAlignment="Top" Width="75" Click="buttonLaplace_Click"/>
        <Button x:Name="buttonNar" Content="WykrywanieNaroznikow" HorizontalAlignment="Left" Margin="90,229,0,0" VerticalAlignment="Top" Width="150" Click="buttonNar_Click"/>

